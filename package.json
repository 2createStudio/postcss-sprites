{
  "name": "@niksy/postcss-sprites",
  "version": "5.0.0",
  "description": "Generate spritesheets from stylesheets",
  "main": "lib/index.js",
  "keywords": [
    "postcss-plugin",
    "postcss",
    "css",
    "spritesheet",
    "spritesmith",
    "sprite",
    "png",
    "svg"
  ],
  "scripts": {
    "test": "ava test/*.js --verbose --serial",
    "build": "babel src/ --out-dir lib/",
    "watch": "babel --watch src/ --out-dir lib/",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "release": "np"
  },
  "repository": "niksy/postcss-sprites",
  "author": "2createStudio",
  "license": "MIT",
  "bugs": "https://github.com/niksy/postcss-sprites/issues",
  "homepage": "https://github.com/niksy/postcss-sprites#readme",
  "devDependencies": {
    "@ava/babel": "^2.0.0",
    "@babel/cli": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "ava": "^3.15.0",
    "np": "^6.5.0",
    "postcss": "^8.1.2"
  },
  "dependencies": {
    "debug": "^4.3.2",
    "fs-extra": "^10.0.0",
    "lodash": "^4.0.0",
    "p-each-series": "^2.2.0",
    "p-filter": "^2.1.0",
    "p-map": "^4.0.0",
    "p-reduce": "^2.1.0",
    "spritesmith": "^3.0.1",
    "svg-sprite": "^1.3.5"
  },
  "peerDependencies": {
    "postcss": "^8.1.2"
  },
  "typings": "./typings.d.ts",
  "engines": {
    "node": ">=12"
  },
  "ava": {
    "babel": true
  },
  "publishConfig": {
    "access": "public"
  }
}
